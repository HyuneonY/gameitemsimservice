generator client {
  provider = "prisma-client-js"
  output   = "./generated/client2"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL2")
}

model Items {
  itemId    Int    @id @default(autoincrement()) @map("itemId")
  itemName  String @map("itemName")
  itemStat  Json   @map("itemStat")
  itemPrice Int    @map("itemPrice")

  @@map("Items")
}
